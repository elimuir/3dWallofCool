tle.styleSheet = myInfobox.description.styleSheet = myInfobox.author.styleSheet = myCSS;					//detailsUrl = infoArray['url'];					myInfobox.author.y = myInfobox.title.y + myInfobox.title.height;					myInfobox.description.y = myInfobox.author.y + myInfobox.author.height;					myInfobox.visible = true;				break;								case "hide":					removeChild(planeArray["infobox"]);				break;			}					}							/**			* Shorten Text			* 			* @param sourceText 			* 	String to be shortened			* @param maxLength			*		Number of charactors to shorten length			* @param ending			* 	Set the ending of the string			* return shorted text string with ending			*			* Ref: http://snipplr.com/view/11141/text-field-limit-with-ellipse/			*/		function truncateText(sourceText, maxLength:Number = 150, ending:String ="...") {			maxLength = maxLength-ending.length;						if(sourceText.length > maxLength){				var lastChar = sourceText.substr(0,maxLength);				var pos = maxLength;								if(lastChar != " ") {					pos = pos+1;				}				return sourceText.substr(0, pos)+ending;			} else {				return sourceText;			}		}				/**			* show tool tip popup 			*/		function showToolTip(evt:Event):void{ 			//Mouse.show();			//myCursor.visible = false;			//stage.removeEventListener(MouseEvent.MOUSE_MOVE, showMouseCursor);			myTooltip = new tooltip();	//Assign movieclip from library			addChildAt(myTooltip, getChildIndex(viewport)+1);			//debug(getChildIndex(viewport))			myTooltip.filters = [dropshadow];			myTooltip.alpha = 0;		// Hide for fade in			myTooltip.tooltiptext.text =  truncateText(unescape(evt.target.title), 50);	//Load and size tooltip text			myTooltip.tooltiptext.width = 200;	//Set tooltip width			myTooltip.tooltiptext.autoSize = TextFieldAutoSize.LEFT;  //Automatically resize block to fit containing text			myTooltip.tooltippointer.y = 0;	//set tooltip pointer to zero			myTooltip.tooltippointer.x = 0;	//set tooltip pointer to zero			myTooltip.bg.width = myTooltip.tooltiptext.width + 10;		//Set size of background to match text field			myTooltip.bg.height = myTooltip.tooltiptext.height + 10;	//Set size of background to match text field			myTooltip.bg.x = -25;	//Position tooltip background			myTooltip.bg.y = -(myTooltip.bg.height + myTooltip.tooltippointer.height - 1);			myTooltip.tooltiptext.x = myTooltip.bg.x + 5;	//center tooltip text within 			myTooltip.tooltiptext.y = myTooltip.bg.y + 5;	//center tooltip text within 			//debug (evt.image.x +","+ evt.image.y);			delayTooltip.addEventListener(TimerEvent.TIMER, fadeInTooltip); 			updateTooltip.addEventListener(TimerEvent.TIMER, placeTooltip);			delayTooltip.start();			updateTooltip.start();		}					/**			*	Hide Tooltip			*/		function hideToolTip(evt:Event){			if(stage.contains(myTooltip)){				updateTooltip.stop();				updateTooltip.removeEventListener(TimerEvent.TIMER, placeTooltip);				delayTooltip.stop();				delayTooltip.removeEventListener(TimerEvent.TIMER, fadeInTooltip); 				removeChild(myTooltip);				myTooltip.tooltiptext.text = "";			}					}					/**			*	Fade In tooltip			*/		function fadeInTooltip(e:TimerEvent){			Tweener.addTween(myTooltip, {alpha:100, time:0.50, transition:"Strong.easeIn"});		}					/**			* Set location of tooltip			*/		function placeTooltip(e:TimerEvent) {			myTooltip.x = mouseX;		// Set location of tooltip centered to mouse			myTooltip.y = mouseY - 5;	// Set location of tooltip right above mouse 			e.updateAfterEvent();		// update using timer not FPS		}					/* End Class */	}			/* End package  */}	
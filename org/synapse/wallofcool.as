myBezierCurvePath.push({ x: -50,  y: 0 });					//myBezierCurvePath.push({ x: 400, y: 500 });						//tweenTo._bezier = myBezierCurvePath;//new Array({x:0, y:500, z:0});						tweenTo.onStart = function(){ 				dispatchEvent(new ImageEvent(ImageEvent.WALL_ZOOMING));  // Dispatch high res image progress 			}			tweenTo.time = 1;//distance(moveObject,tweenObject)/100;			tweenTo.transition = "easeOut";			tweenTo.onUpdate =  zoomUpdate;			tweenTo.onUpdateParams = new Array(moveObject);			Tweener.addTween(moveObject, tweenTo);					}					/**			*  function to update wall location variables; used by tween to update movement of wall by camRender()			*/		function zoomUpdate(trg:Object):void {			wallLocationX = trg.x;			wallLocationY = trg.y;			wallLocationZ = trg.z;		}						function loadHQImage(trg:Object = null):void{						Tweener.addTween(trg._plane, {alpha:0, time:0.5, transition:"Strong.easeIn"});	// Fade out low-res image							// Create movieclip to hold imported elements			var movieClip = new MovieClip();					//movieClip.graphics.lineStyle(10, 0xFFFFFF);				//movieClip.graphics.beginFill(0xFF0000);				//movieClip.graphics.drawRoundRectComplex(0,0,_thmbnailWidth,_thmbnailHeight,8,8,8,8);				movieClip.graphics.drawRect(0,0,largeImage.width,largeImage.height);				movieClip.graphics.endFill();				//movieClip.alpha = 0.5;							// Create Material for Movie using MovieClip created above			var _material = new MovieMaterial(movieClip, true);				_material.animated=true;     			_material.doubleSided=true;				_material.smooth=true;				_material.interactive=true;							//Create Image loader animation			var myImageAnim = new imageloaderAnim();				movieClip.addChild(myImageAnim);								// Display loader				//myImageAnim.x = (movieClip.width/2) - (myImageAnim.width/2);	// Center movie to stage				//myImageAnim.y = (movieClip.height/2) - (myImageAnim.height/2);	// Center movie to stage				centerTo(myImageAnim, movieClip);									// Center movie to stage								// Create Plane Obj			var hires_plane = new Plane(_material, thumbImage.width, thumbImage.height, 3, 3);				hires_plane.useOwnContainer = true;												// Enables setting alpha/opacity for plane materials 				//_plane.alpha = 100;															// Set plane alpha				hires_plane.name = "hires_plane";				planeArray["hires-container"] = hires_plane;				//_plane.addEventListener(InteractiveScene3DEvent.OBJECT_OVER, onMouseOver);	// Add Mouse Over handler				//_plane.addEventListener(InteractiveScene3DEvent.OBJECT_OUT, onMouseOut);	// Add Mouse Out handler				hires_plane.addEventListener(InteractiveScene3DEvent.OBJECT_PRESS, function(e:InteractiveScene3DEvent){  // Add Mouse Click handler					zoomTo("out");					Tweener.addTween(trg._plane, {alpha:100, time:500, transition:"Strong.easeIn"});				});								hires_plane.copyTransform(trg._plane);	//Copy existing location of thumbnail plane				hires_plane.z = hires_plane.z - 10;		//Set high res image in front of thumbnail						var file = imgHolders[trg.name.slice(8,trg.name.length)].hiRes;	// Get filename from imgHolders Array using number for targets name param						_imageLoader = new Loader();							// Add progress handler 			_imageLoader.contentLoaderInfo.addEventListener(ProgressEvent.PROGRESS, function(e:ProgressEvent){  				dispatchEvent(new ImageEvent(ImageEvent.IMAGE_LARGE_PROGRESS)); // Dispatch high res image progress 				debug(Math.round((e.bytesLoaded/e.bytesTotal) * 100));			});			_imageLoader.contentLoaderInfo.addEventListener(ProgressEvent.PROGRESS, function(e:ProgressEvent){  							});							// Create IOError Handler			_imageLoader.contentLoaderInfo.addEventListener(IOErrorEvent.IO_ERROR, function(e:IOErrorEvent){				debug("Error: "+ e.text);			// Display error to debugger				movieClip.removeChild(myImageAnim);	// Remove loading animation				Tweener.addTween(trg._plane, {alpha:100, time:0.25, transition:"Strong.easeIn"});			});							// Add load